---
import Logo from "../components/logo.astro";
import Github from "./svg/github.astro";
import Linkedin from './svg/linkedin.astro'
import Malt from './svg/malt.astro'
import Notion from './svg/notion.astro'
import Resume from './svg/resume.astro'
---

<section
  class="text-gray-700 bg-white border-t sm:mt-20 dark:bg-neutral-950 border-neutral-200 dark:border-neutral-800"
>
  <div
    class="container flex flex-col items-center py-8 mx-auto px-7 max-w-7xl sm:flex-row"
  >
    <Logo />
    <!-- <p
      class="mt-4 text-sm text-neutral-700 dark:text-neutral-100 sm:ml-4 sm:pl-4 sm:border-l sm:border-neutral-300 dark:sm:border-neutral-700 sm:mt-0"
    >
      Â© {new Date().getFullYear()} Aria
    </p> -->
    <span
      class="inline-flex justify-center mt-4 space-x-4 sm:ml-auto sm:mt-0 sm:justify-start"
    >

      <a
        href="https://github.com/paul-louyot"
        target="_blank"
        class="text-neutral-500 dark:text-neutral-400 hover:text-neutral-900 dark:hover:text-white"
      >
        <span class="sr-only">GitHub</span>
        <Github/>
      </a>

      <a
        href="https://www.linkedin.com/in/paul-louyot"
        target="_blank"
        class="text-neutral-500 dark:text-neutral-400 hover:text-neutral-900 dark:hover:text-white"
      >
        <span class="sr-only">Linkedin</span>
        <Linkedin/>
      </a>

      <a
        href="https://www.malt.fr/profile/paullouyot"
        target="_blank"
        class="text-neutral-500 dark:text-neutral-400 hover:text-neutral-900 dark:hover:text-white"
      >
        <span class="sr-only">Malt</span>
        <Malt/>
      </a>

      <a
        href="https://inexpensive-hose-383.notion.site/Paul-Louyot-D-veloppeur-freelance-d87dbb1051044f2497ccdca7e10fae88"
        target="_blank"
        class="text-neutral-500 dark:text-neutral-400 hover:text-neutral-900 dark:hover:text-white"
      >
        <span class="sr-only">Notion</span>
        <Notion/>
      </a>

      <a
        href="https://registry.jsonresume.org/paul-louyot?theme=elegant"
        target="_blank"
        class="text-neutral-500 dark:text-neutral-400 hover:text-neutral-900 dark:hover:text-white"
      >
        <span class="sr-only">Resume</span>
        <Resume/>
      </a>

    </span>
  </div>
</section>
